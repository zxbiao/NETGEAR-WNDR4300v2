config NAT_GIT_TREEISH
	string
	prompt "treeish of dni-nat2.5 git repository"
	default "nat2.5-dni7"
	help
           Input the treeish of dni-nat2.5 git repository which you want to build

config NAT_GIT_REPOSITORY
	string
	prompt "name of dni-nat2.5 git repository"
	default "dni-nat2.5.git"
