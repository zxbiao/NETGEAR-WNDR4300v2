config NETGEAR_REJECT_GIT_URL
	string
	prompt "url of kmod-netgear-reject git repository"
	default "ssh://dniserver/scm/dnigit/common/kmods/kmod-netgear-reject.git"
	help
	   Input the url of kmod-netgear-reject git repository which you want to build
